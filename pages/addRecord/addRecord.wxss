page {
  height: 100%;
}

#pageContainer {
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: flex-start;
}

#leftContainer {
  width: 200rpx;
  height: 100%;
  margin-right: 50rpx;
  padding-top: 10rpx;
}

#leftContainer button {
  margin-bottom: 24rpx;
}

#subItemsList {
  width: 430rpx;
  height: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  padding-top: 10rpx;
}

#scanBtn {
  background: #cbaa50 url('data:image/png;base64,') 10rpx center no-repeat;
  background-size: 30rpx 30rpx;
}

#changeToCheckpointBtn {
  background: #99c927 url('data:image/png;base64,') 10rpx center no-repeat;
  background-size: 30rpx 30rpx;
}

#changeToEntityBtn {
  background: #99c927 url('data:image/png;base64,') 10rpx center no-repeat;
  background-size: 30rpx 30rpx;
}

#examineFinishBtn {
  background-color: #2fa0d4;
  color: #fff;
  font-size: 30rpx;
  height: 75rpx;
}

#scanBtn, #changeToCheckpointBtn, #changeToEntityBtn {
  color: #fff;
  text-indent: 30rpx;
  font-size: 30rpx;
  height: 75rpx;
  padding: 0;
}

.parent-data-item {
  padding: 14px 14rpx;
  background-color: #f2f2f2;
  font-size: 30rpx;
  position: relative;
}

.parent-data-item-selected {
  background-color: #2fa0d4;
}

.parent-item-unfinished-icon {
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  border-radius: 50%; 
  top: 13rpx;
  right: 13rpx;
  color: white;
  font-size: 18rpx;
  font-weight: bold;
  line-height: 30rpx;
  text-align: center;
  background-color: red;
}

.parent-item-index, .parent-item-name {
  display: block;
  line-height: 40rpx;
}

.parent-item-name {
  margin-top: 4rpx;
}

.parent-item-index {
  display: block;
  color: #6e6e6e;
  font-size: 26rpx;
  margin-top: 4rpx;
}

.parent-item-name {
  color: #2fa0d4;
  font-size: 30rpx;
}

.parent-data-item-selected > .parent-item-index,
.parent-data-item-selected > .parent-item-name {
  color: #fff !important;
}

.sub-data-item-box {
  border: 2rpx solid #2fa0d4;
  border-radius: 12rpx;
  display: flex;
  justify-content: center;
  margin-bottom: 15rpx;
}

.sub-data-item {
  width: 420rpx;
  padding: 15rpx;
}

.sub-data-item-selected {
  border: 5rpx solid #2fa0d4 !important;
}

.sub-item-name-box {
  display: flex;
  justify-content: space-between;
}

.sub-item-name {
  font-size: 28rpx;
  color: #3e3e3e;
  width: 65%;
  line-height: 40rpx;
}

.sub-item-count {
  font-size: 20rpx;
  color: #999;
  width: 32%;
  line-height: 40rpx;
  text-align: right;
}

.sub-item-passed-count, .sub-item-unpassed-count {
  font-size: 20rpx;
  color: #999;
  margin-top: 30rpx;
}

.sub-item-unpassed-count {
  margin-left: 50rpx;
}

.sub-item-btn-box {
  display: flex;
  justify-content: space-between;
  margin-top: 20rpx;
}

.sub-item-white-btn, .sub-item-filled-btn {
  width: 120rpx;
  height: 60rpx;
  border: 1rpx solid #2fa0d4;
  border-radius: 12rpx;
  font-size: 22rpx;
  padding: 0;
}

.sub-item-white-btn {
  background-color: #fff;
  color: #2fa0d4;
}

.sub-item-filled-btn {
  background-color: #2fa0d4;
  color: #fff;
}
